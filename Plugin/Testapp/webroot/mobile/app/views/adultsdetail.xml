<Alloy>
			<Model src="Adult" instance="true" id="thingDetail" />
			<Window id="detail" model="$.thingDetail" dataTransform="dataTransformation" layout="vertical"> 
				<TextField id="name" datid = "{$.thingDetail.id}" value="{$.thingDetail.name}"  hintText="Name" ></TextField>
				<TextField id="homeloc_lat" hintText="homeloc_lat" value="{$.thingDetail.homeloc_lat}" ></TextField>
					<TextField id="homeloc_lng" hintText="homeloc_lng" value="{$.thingDetail.homeloc_lng}" ></TextField>
					<View id="map" module="ti.map" width="100%" height="20%"/>
					<TextField id="photo" hintText="photo" value="{$.thingDetail.photo}" ></TextField>
					<ImageView id="photoimage" image="{$.thingDetail.photo}" width="10" height="10" ></ImageView>
					
				
				<Button id="savebtn">Save</Button>
				<Button id="cancelbtn">Cancel</Button>
			</Window>
		</Alloy>
		